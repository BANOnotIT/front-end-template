{
  "extends": "next/core-web-vitals",
  "plugins": ["simple-import-sort", "react"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "sort-imports": "off",
    "react/jsx-curly-brace-presence": [
      "warn",
      { "props": "never", "children": "never" }
    ]
  }
}
